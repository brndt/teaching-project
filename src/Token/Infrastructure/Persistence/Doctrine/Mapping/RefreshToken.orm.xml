<doctrine-mapping>
    <entity name="LaSalle\StudentTeacher\Token\Domain\Aggregate\RefreshToken" table="refresh_tokens">
        <id name="id" type="uuid"/>
        <field name="refreshToken" type="refreshTokenString" unique="true" nullable="false"/>
        <field name="userId" type="uuid" nullable="false"/>
        <field name="expirationDate" type="datetime" nullable="false"/>
    </entity>
</doctrine-mapping>